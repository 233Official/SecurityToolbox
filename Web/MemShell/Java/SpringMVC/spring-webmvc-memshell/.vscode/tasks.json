{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Maven Compile",
        "type": "shell",
        "command": "mvn clean compile",
        "group": {
          "kind": "build",
          "isDefault": false
        },
        "problemMatcher": ["$javac"]
      },
      {
        "label": "Maven Package",
        "type": "shell",
        "command": "mvn clean package",
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": ["$javac"]
      }
    ]
  }
  